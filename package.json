  "scripts": {
    "build": "gulp clean-build",
    "build-ci": "gulp ci",
    "build-storybook": "build-storybook -s public",
    "build:base": "gulp clean-build --silent",
    "build:chromium": "gulp clean-build --chrome --silent",
    "build:e2e": "gulp clean-build --e2e",
    "build:firefox": "gulp clean-build --fx --silent",
    "build:gecko": "gulp clean-build --android --silent",
    "build:ios": "gulp clean-build --ios --silent",
    "compile:contracts": "./scripts/contracts.sh",
    "lint": "eslint src/ --ext .ts,.tsx --fix",
    "lint:report": "eslint src/ --ext .ts,.tsx --format junit -o reports/junit/eslint-results.xml",
    "lint:typecoverage": "type-coverage --strict --ignore-catch --ignore-files src/plugins/Wallet/contracts/** --ignore-files **/__tests__/** --ignore-files **\\__tests__\\** --detail --at-least 98",
    "go": "gulp interactive-watch --silent",
    "start": "gulp watch",
    "start:chromium": "gulp watch --chrome",
    "start:e2e": "gulp watch --e2e",
    "start:firefox": "gulp watch --fx",
    "start:gecko": "gulp watch --android",
    "start:ios": "gulp watch --ios",
    "storybook": "start-storybook -p 9009 -s public --quiet",
    "profile:build": "yarn build --profile",
    "profile:start": "yarn start --profile",
    "test": "jest --verbose --watch",
    "test:ci": "jest --verbose --ci --collectCoverage=true --reporters=default --reporters=jest-junit -w 1 --forceExit",
    "test:e2e": "jest --verbose --runInBand --config=jest-e2e.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --ext .ts,.tsx --fix"
    ]
  },
